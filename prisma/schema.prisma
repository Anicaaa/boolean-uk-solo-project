generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider          = "postgresql"
    url               = env("DATABASE_URL")
    shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model User {
    id              Int      @id @default(autoincrement())
    name            String   @unique
    email           String   @unique
    password        String
    confirmPassword String
    createdAt       DateTime @default(now())
    updatedAt       DateTime @updatedAt
}
